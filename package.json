{
  "name": "games-of-strategy",
  "version": "0.0.0",
  "description": "A web app for game theory",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "cpx": "^1.5.0",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "run-s install:*",
    "install:fe": "cd gos-fe && npm install",
    "install:be": "cd gos-be && dotnet restore",
    "build": "run-s build:*",
    "build:fe": "cd gos-fe && npm run build",
    "postbuild:fe": "cpx \"gos-fe/dist/apps/client/**/*.*\" \"gos-be/GOS.Api/wwwroot/\"",
    "build:be": "cd gos-be && dotnet build --configuration release",
    "start": "run-p start:*",
    "start:fe": "cd gos-fe && npm run start",
    "start:be": "dotnet watch --project gos-be/GOS.Api run --no-launch-profile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SoftwareKater/games-of-strategy.git"
  },
  "author": "Johannes Katzer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SoftwareKater/games-of-strategy/issues"
  },
  "homepage": "https://github.com/SoftwareKater/games-of-strategy#readme"
}
